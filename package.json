{
  "name": "ai-sales-agent-root",
  "version": "1.0.0",
  "private": true,
  "description": "AI-powered B2B prospecting SaaS platform",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "build:web": "npm install && cd apps/web && npm run build",
    "start:web": "cd apps/web && npm start",
    "vercel-build": "npm run build:web",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset"
  },
  "devDependencies": {
    "@turbo/gen": "^1.10.12",
    "@types/node": "^20.5.2",
    "eslint": "^8.48.0",
    "prettier": "^3.0.0",
    "prisma": "^5.7.0",
    "tsx": "^4.6.2",
    "turbo": "^1.10.12",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0"
  },
  "packageManager": "npm@10.0.0",
  "engines": {
    "node": ">=20.0.0"
  }
}