{
  "name": "ai-sales-agent",
  "version": "1.0.0",
  "private": true,
  "description": "AI-powered B2B prospecting SaaS platform",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "cd apps/web && npm run dev",
    "build": "cd apps/web && npm run build",
    "start": "cd apps/web && npm run start",
    "lint": "cd apps/web && npm run lint",
    "typecheck": "cd apps/web && npm run typecheck",
    "db:generate": "cd apps/web && npm run db:generate",
    "db:push": "cd apps/web && npm run db:push",
    "db:migrate": "cd apps/web && npm run db:migrate",
    "db:studio": "cd apps/web && npm run db:studio",
    "verify": "bash scripts/verify-deployment.sh"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yoprobotics/ai-sales-agent.git"
  },
  "keywords": [
    "saas",
    "b2b",
    "sales",
    "prospecting",
    "ai",
    "crm",
    "nextjs",
    "typescript"
  ],
  "author": "YoProbotics <yoprobotics@gmail.com>",
  "license": "PROPRIETARY",
  "bugs": {
    "url": "https://github.com/yoprobotics/ai-sales-agent/issues"
  },
  "homepage": "https://github.com/yoprobotics/ai-sales-agent#readme"
}
